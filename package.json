{
  "name": "healtek",
  "version": "1.0.0",
  "description": "Production-ready authentication system using Node.js, Express, and MongoDB",
  "main": "backend/src/server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/src/server.js",
    "dev": "cross-env NODE_OPTIONS=--experimental-vm-modules nodemon --watch backend backend/src/server.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest --coverage",
    "test:integration": "node backend/test/integration.test.mjs",
    "migrate:profiles": "node -r esm scripts/migrate-profiles.js"
  },
  "keywords": [
    "auth",
    "jwt",
    "node",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/nodemailer": "^7.0.4",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.3.1",
    "ioredis": "^5.3.2",
    "joi": "^17.9.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "nodemailer": "^7.0.12",
    "rate-limit-redis": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.8.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.6.1",
    "mongodb-memory-server": "^8.12.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  },
  "optionalDependencies": {
    "cors": "^2.8.6",
    "express-mongo-sanitize": "^2.2.0",
    "helmet": "^6.0.1",
    "xss-clean": "^0.1.1"
  }
}
